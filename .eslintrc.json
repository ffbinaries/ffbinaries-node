{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": false
  },
  "rules": {
    "semi": 2,
    "no-console": 0,
    "no-var": 0,
    "prefer-template": 0,
    "prefer-arrow-callback": 0,
    "comma-dangle": 0,
    "no-path-concat": 1,
    "vars-on-top": 0,
    "func-names": 0,
    "prefer-numeric-literals": 0,
    "prefer-destructuring": 0,
    "consistent-return": 1,
    "max-len": 0,
    "no-param-reassign": 0,
    "no-empty": 0,
    "object-shorthand": 0,
    "no-mixed-operators": 0,
    "use-isnan": 0,
    "eqeqeq": 0,
    "no-useless-concat": 1,
    "object-curly-newline": 1
  }
}
